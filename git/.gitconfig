# This is Git's per-user configuration file.
[user]
#	name = Rin
#	email = rin@rin.systems
[core]
	editor = vim

[merge]
	conflictstyle = diff3
[alias]
	tree = log --graph --decorate --abbrev-commit --all --oneline
[safe]

[branch]
  autosetuprebase = always

[rebase]
  autoStash = true
[pull]
  rebase = true
